{
    "files.exclude": {
        "dist/**": true,
        "dist-newstyle/**": true,
        ".ghc.environment.*": true,
        ".gitattributes": true,
        ".env/**": true
    },
    "files.watcherExclude": {
        "**/.out/**": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.env/**": true,
        "**/dist/**": true,
        "**/dist-newstyle/**": true
    },
    "hic.arguments": "--lsp --cwd src"
}